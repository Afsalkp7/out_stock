    <script>

        const xValues = ["DAY1", "DAY2", "DAY3", "DAY4", "DAY5", "DAY6", "DAY7"];
        const yValues = [{{ weekOrdersAmount }}];
        const barColors = ["black", "gray", "black", "gray", "black", "gray", "black"];

        new Chart("myChart", {
            type: "bar",
            data: {
                labels: xValues,
                datasets: [{
                    backgroundColor: barColors,
                    data: yValues
                }]
            },
            options: {
                legend: { display: false },
                title: {
                    display: true
                }
            }
        });
    </script>
